    "    r"\dfrac{d}{d x}\,\arccos x": r"-\dfrac{1}{\sqrt{1-x^2}}",",
    "",
    "    # Regole di derivazione",
    "    r"\dfrac{d}{d x}\,[k\cdot f(x)]": r"k\cdot f'(x)",",
    "    r"\dfrac{d}{d x}\,[f(x)+g(x)]": r"f'(x)+g'(x)",",
    "    r"\dfrac{d}{d x}\,[f(x)\cdot g(x)]": r"f'(x)\cdot g(x)+f(x)\cdot g'(x)",",
