    "}",
    "",
    "derivatives = {",
    "    # Potenze di x",
    "    r"\dfrac{d}{d x}\,k": r"0",",
    "    r"\dfrac{d}{d x}\,x^a": r"ax^{a-1},\quad a\in\mathbb{R}",",
    "    r"\dfrac{d}{d x}\,x": r"1",",
